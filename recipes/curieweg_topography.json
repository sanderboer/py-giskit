{
  "name": "Curieweg Topography & 3D",
  "description": "Download topographic maps and 3D data for Curieweg area",
  "location": {
    "type": "address",
    "value": "Curieweg 7a, Spijkenisse, Netherlands",
    "radius": 1000,
    "crs": "EPSG:4326"
  },
  "datasets": [
    {
      "provider": "pdok",
      "service": "top10nl",
      "layers": [
        "gebouw",
        "wegdeel",
        "waterdeel",
        "terrein"
      ]
    },
    {
      "provider": "pdok",
      "service": "3d-basisvoorziening",
      "layers": [
        "gebouw_3d",
        "terrein_3d"
      ]
    },
    {
      "provider": "pdok",
      "service": "brt-hydrografie",
      "layers": [
        "watervlakken",
        "waterlopen"
      ]
    }
  ],
  "output": {
    "path": "data/curieweg_topography.gpkg",
    "format": "gpkg",
    "crs": "EPSG:28992",
    "overwrite": true,
    "layer_prefix": "topo_"
  }
}
